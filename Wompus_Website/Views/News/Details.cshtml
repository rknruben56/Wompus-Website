@model Wompus_Website.Models.News

@{
    ViewBag.Title = "Details";
}
<div class="body div-content">
    <h2>
        @Model.title.ToString()
    </h2>
    <p class="body description-font">
        @Html.Raw(Model.Description.ToString())
    </p>
    <p class="body publish-time">
        @Html.DisplayFor(modelModel => Model.PublishTime)
    </p>
</div>

